      "expression": "((new Date(+this.serviceRequest.ItemSoldOn + (new Date((365.25 / 12) * (24 * 3600 * 1000)) * ((this.serviceRequest.WarrantyLength || 0) + (this.serviceRequest.ManufacturerWarrantyLength || 0))))) >= (new Date()))"
    },
    {
      "expression": "this.serviceRequest.Id === 0"
    },
    {
      "expression": "this.CustomerSearch.Type === 'Invoice'"
    },
    {
      "expression": "this.serviceRequest.ResolutionPrimaryCharge"
    },
    {
      "expression": "this.serviceRequest.ResolutionPrimaryCharge === 'Deliverer' || this.serviceRequest.ResolutionPrimaryCharge === 'Supplier'"
    },
    {
      "expression": "this.OutstandingBalance > 0 || this.serviceRequest.PaymentBalance> 0"
    },
    {
      "expression": "this.isItemBer"
    },
    {
      "expression": "this.serviceRequest.Evaluation"
    },
    {
      "expression": "this.serviceRequest.DepositRequired > 0
&& (!this.serviceRequest.PaymentBalance || (this.serviceRequest.PaymentBalance < this.serviceRequest.DepositRequired))"
    },
    {
      "expression": "this.OutstandingBalance > 0 || outStandingDeposit(this) > 0"
    },
    {
      "expression": "this.serviceRequest.IsClosed"
    },
    {
      "expression": "outStandingDeposit(this) === 0"
    },
    {
      "expression": "outStandingDeposit(this) === 0"
    }
