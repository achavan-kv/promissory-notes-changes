    }
    if (scope.serviceRequest.ResolutionDelivererToCharge) {
        delete scope.serviceRequest.ResolutionDelivererToCharge; // clean select2
    }
    if (scope.serviceRequest.ResolutionCategory) {
        delete scope.serviceRequest.ResolutionCategory; // clean select2
    }
};

var chargeTos = function(scope, list) {
 scope.MasterData.ServiceChargeTos = list;
 };

var outStandingDeposit = function (scope) {
   if (scope.serviceRequest.PaymentBalance >= scope.serviceRequest.DepositRequired) {
           return 0;
      } else {
            return (scope.serviceRequest.DepositRequired - scope.serviceRequest.PaymentBalance);
     }
};"
