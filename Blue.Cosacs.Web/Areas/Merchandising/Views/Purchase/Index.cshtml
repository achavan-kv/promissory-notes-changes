@model string
@{
    ViewBag.Title = "Purchase Order Search";
}
<div id="facetSearchContainer" data-module="merchandising/search/initSearch" ng-controller="SearchController" class="facetsearch-container">
    <facetsearch data-buttons="@ViewBag.buttons" data-results="@Model"
                 data-results-template="/Static/js/merchandising/purchase/templates/searchResult.html"
                 data-search-url="/Merchandising/Purchase/SearchInstant"
                 data-history-url="/Merchandising/Purchase"
                 data-date-fields='{"ExpectedDeliveryDate": "Estimated Delivery Date", "ExpiryDate": "Expiry Date"}'
                 data-convert-dates='false'
                 data-field-labels='{"Vendor": "Vendor","ReceivingLocation": "Receiving Location","Status": "Status"}'>
    </facetsearch>
</div>
