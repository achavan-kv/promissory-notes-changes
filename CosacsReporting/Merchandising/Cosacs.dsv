            <xs:element name="PriceDifferential" msprop:design-time-name="1f228168-dbaa-483f-a02d-68de57d30da1" msprop:IsLogical="True" msprop:FriendlyName="PriceDifferential" msprop:DbTableName="PriceDifferential" msprop:TableType="View" msprop:Description="" msprop:QueryDefinition="SELECT        &#xD;&#xA;    co.Id,&#xD;&#xA;    co.SaleType,&#xD;&#xA;    CONVERT(INT, CONVERT(VARCHAR(8), DATEADD(mi, DATEDIFF(mi, GETUTCDATE(), GETDATE()), co.TransactionDate), 112)) AS TransactionDate, &#xD;&#xA;    co.Quantity * co.Price AS Price, &#xD;&#xA;    co.Quantity * (co.Price - cp.AverageWeightedCost) AS GrossProfit, &#xD;&#xA;    ISNULL(division.Tag, '(Unknown Division)') AS Division, &#xD;&#xA;    ISNULL(Department.Tag, '(Unknown Department)') AS Department, &#xD;&#xA;    (co.Price - co2.CashPrice) * co.Quantity AS PriceDifferential, &#xD;&#xA;    (co.Price - co2.CashPrice) * co.Quantity AS PriceDifferentialGrossProfit&#xD;&#xA;FROM Merchandising.CintOrder AS co &#xD;&#xA;INNER JOIN Merchandising.Product AS p &#xD;&#xA;    ON co.Sku = p.SKU &#xD;&#xA;    AND co.Type IN ('Return', 'Delivery', 'Redelivery', 'Repossession') &#xD;&#xA;INNER JOIN Merchandising.CurrentCostPriceView AS cp &#xD;&#xA;    ON p.Id = cp.ProductId &#xD;&#xA;LEFT JOIN Merchandising.ProductHierarchyView AS division &#xD;&#xA;    ON division.ProductId = p.Id &#xD;&#xA;    AND division.[Level] = 'Division' &#xD;&#xA;LEFT JOIN Merchandising.ProductHierarchyView AS Department &#xD;&#xA;    ON Department.ProductId = p.Id &#xD;&#xA;    AND Department.[Level] = 'Department' &#xD;&#xA;INNER JOIN Merchandising.Dates AS d &#xD;&#xA;    ON CONVERT(INT, CONVERT(VARCHAR(8), DATEADD(mi, DATEDIFF(mi, GETUTCDATE(), GETDATE()), co.TransactionDate), 112)) = d.DateKey &#xD;&#xA;INNER JOIN Merchandising.CintOrder AS co2 &#xD;&#xA;    ON co.Id != co2.Id &#xD;&#xA;    AND co.PrimaryReference = co2.PrimaryReference &#xD;&#xA;    AND co.StockLocation = co2.StockLocation &#xD;&#xA;    AND co.Sku = co2.Sku &#xD;&#xA;    AND co2.[Type] = 'RegularOrder' &#xD;&#xA;    AND &#xD;&#xA;&#x9;(&#xD;&#xA;&#x9;&#x9;(&#xD;&#xA;&#x9;&#x9;&#x9;co2.ReferenceType = 'invoice' &#xD;&#xA;&#x9;&#x9;&#x9;AND co2.SecondaryReference = co.SecondaryReference&#xD;&#xA;&#x9;&#x9;)&#xD;&#xA;&#x9;&#x9;OR co2.ReferenceType != 'invoice'&#xD;&#xA;&#x9;) &#xD;&#xA;    AND co2.TransactionDate = &#xD;&#xA;&#x9;(&#xD;&#xA;        SELECT MAX(TransactionDate) AS Expr1&#xD;&#xA;        FROM Merchandising.CintOrder AS co3&#xD;&#xA;        WHERE [Type] = 'RegularOrder'&#xD;&#xA;            AND PrimaryReference = co.PrimaryReference&#xD;&#xA;            AND StockLocation = co.StockLocation&#xD;&#xA;&#x9;&#x9;&#x9;AND Id != co.Id &#xD;&#xA;&#x9;&#x9;&#x9;AND Sku = co.Sku&#xD;&#xA;&#x9;&#x9;&#x9;AND&#xD;&#xA;&#x9;&#x9;&#x9;(&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;(&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;ReferenceType = 'invoice'&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;AND SecondaryReference = co.SecondaryReference&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;)&#xD;&#xA;&#x9;&#x9;&#x9;&#x9;OR ReferenceType != 'invoice'&#xD;&#xA;&#x9;&#x9;&#x9;)&#xD;&#xA;    )&#xD;&#xA;WHERE co.SaleType = 'Credit'" msprop:QueryBuilder="GenericQueryBuilder">
