                  <xs:element name="GrossProfit" msdata:ReadOnly="true" msprop:design-time-name="43b95c1b-0d8e-4cf5-8fd3-cc989b4cb1de" msprop:DbColumnName="GrossProfit" msprop:FriendlyName="GrossProfit" type="xs:double" minOccurs="0" />
