            <xs:element name="WarrantySales" msprop:design-time-name="4e96e9cf-285e-4db8-b651-0539891d10a3" msprop:IsLogical="True" msprop:FriendlyName="WarrantySales" msprop:DbTableName="WarrantySales" msprop:TableType="View" msprop:Description="" msprop:QueryDefinition="SELECT        CONVERT(VARCHAR, d.acctno) + CONVERT(VARCHAR, d.datetrans, 109) + CONVERT(VARCHAR, d.agrmtno) + CONVERT(VARCHAR, w.Id) AS Id, CONVERT(INT, CONVERT(VARCHAR(8), d.datetrans, 112)) &#xD;&#xA;                         AS TransactionDate, CASE SUBSTRING(d .acctno, 4, 1) WHEN '5' THEN 'Cash' WHEN '4' THEN 'Cash' ELSE 'Credit' END AS SaleType, COALESCE (t.Name, '(Unknown Division)') AS Division, &#xD;&#xA;                         CASE w.TypeCode WHEN 'E' THEN 'Extended' WHEN 'F' THEN 'Free' ELSE 'Unknown' END AS Type, CASE WHEN c.agrmttaxtype = 'I' AND c.taxtype = 'I' THEN d .transvalue / (1 + w.taxrate / 100) &#xD;&#xA;                         ELSE d .transvalue END AS Price, (CASE WHEN c.agrmttaxtype = 'I' AND c.taxtype = 'I' THEN d .transvalue / (1 + w.taxrate / 100) ELSE d .transvalue END) - d.quantity * COALESCE (branchprice.CostPrice, &#xD;&#xA;                         fasciaprice.CostPrice, allprice.CostPrice) AS GrossProfit&#xD;&#xA;FROM            country AS c CROSS JOIN&#xD;&#xA;                         delivery AS d INNER JOIN&#xD;&#xA;                         branch AS b ON b.branchno = d.branchno INNER JOIN&#xD;&#xA;                         StockInfo AS i ON i.Id = d.ItemID INNER JOIN&#xD;&#xA;                         Warranty.Warranty AS w ON w.Number = i.itemno INNER JOIN&#xD;&#xA;                         Merchandising.Dates AS dates ON CONVERT(INT, CONVERT(VARCHAR(8), d.datetrans, 112)) = dates.DateKey LEFT OUTER JOIN&#xD;&#xA;                         Warranty.WarrantyPrice AS branchprice ON branchprice.WarrantyId = w.Id AND branchprice.BranchNumber = b.branchno AND branchprice.EffectiveDate &lt;= d.datetrans LEFT OUTER JOIN&#xD;&#xA;                         Warranty.WarrantyPrice AS fasciaprice ON fasciaprice.WarrantyId = w.Id AND fasciaprice.BranchType = b.StoreType AND fasciaprice.EffectiveDate &lt;= d.datetrans AND fasciaprice.BranchNumber IS NULL &#xD;&#xA;                         LEFT OUTER JOIN&#xD;&#xA;                         Warranty.WarrantyPrice AS allprice ON allprice.WarrantyId = w.Id AND allprice.BranchType IS NULL AND allprice.BranchNumber IS NULL AND allprice.EffectiveDate &lt;= d.datetrans LEFT OUTER JOIN&#xD;&#xA;                         Warranty.WarrantyTags AS wt ON wt.WarrantyId = w.Id AND wt.LevelId =&#xD;&#xA;                             (SELECT        MIN(Id) AS Expr1&#xD;&#xA;                               FROM            Warranty.[Level]) LEFT OUTER JOIN&#xD;&#xA;                         Warranty.Tag AS t ON t.Id = wt.TagId&#xD;&#xA;WHERE        (branchprice.EffectiveDate IS NULL OR&#xD;&#xA;                         branchprice.EffectiveDate IN&#xD;&#xA;                             (SELECT        MAX(EffectiveDate) AS Expr1&#xD;&#xA;                               FROM            Warranty.WarrantyPrice AS m&#xD;&#xA;                               WHERE        (Id = branchprice.Id) AND (EffectiveDate &lt;= d.datetrans))) AND (fasciaprice.EffectiveDate IS NULL OR&#xD;&#xA;                         fasciaprice.EffectiveDate IN&#xD;&#xA;                             (SELECT        MAX(EffectiveDate) AS Expr1&#xD;&#xA;                               FROM            Warranty.WarrantyPrice AS m&#xD;&#xA;                               WHERE        (Id = fasciaprice.Id) AND (EffectiveDate &lt;= d.datetrans))) AND (allprice.EffectiveDate IS NULL OR&#xD;&#xA;                         allprice.EffectiveDate IN&#xD;&#xA;                             (SELECT        MAX(EffectiveDate) AS Expr1&#xD;&#xA;                               FROM            Warranty.WarrantyPrice AS m&#xD;&#xA;                               WHERE        (Id = allprice.Id) AND (EffectiveDate &lt;= d.datetrans)))" msprop:QueryBuilder="SpecificQueryBuilder">
