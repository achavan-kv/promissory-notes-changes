            <xs:element name="Merchandising_CintOrder" msprop:design-time-name="9232e8df-5ffd-4593-8094-fd72f9b05852" msprop:IsLogical="True" msprop:FriendlyName="MerchandiseSales" msprop:DbTableName="MerchandiseSales" msprop:TableType="View" msprop:QueryDefinition="SELECT        co.Id, co.SaleType, CONVERT(INT, CONVERT(VARCHAR(8), DATEADD(mi, DATEDIFF(mi, GETUTCDATE(), GETDATE()), co.TransactionDate), 112)) AS TransactionDate, co.Price * co.Quantity AS Price, &#xD;&#xA;                         (co.Price - cp.AverageWeightedCost) * co.Quantity AS GrossProfit, ISNULL(division.Tag, '(Unknown Division)') AS Division, ISNULL(Department.Tag, '(Unknown Department)') AS Department&#xD;&#xA;FROM            Merchandising.CintOrder AS co INNER JOIN&#xD;&#xA;                         Merchandising.Product AS p ON co.Sku = p.SKU AND co.Type IN ('Return', 'Delivery', 'Redelivery', 'Repossession') INNER JOIN&#xD;&#xA;                         Merchandising.CurrentCostPriceView AS cp ON p.Id = cp.ProductId LEFT OUTER JOIN&#xD;&#xA;                         Merchandising.ProductHierarchyView AS division ON division.ProductId = p.Id AND division.[Level] = 'Division' LEFT OUTER JOIN&#xD;&#xA;                         Merchandising.ProductHierarchyView AS Department ON Department.ProductId = p.Id AND Department.[Level] = 'Department' INNER JOIN&#xD;&#xA;                         Merchandising.Dates AS d ON CONVERT(INT, CONVERT(VARCHAR(8), DATEADD(mi, DATEDIFF(mi, GETUTCDATE(), GETDATE()), co.TransactionDate), 112)) = d.DateKey" msprop:QueryBuilder="SpecificQueryBuilder">
              <xs:complexType>
                <xs:sequence>
                  <xs:element name="SaleType" msprop:design-time-name="9823f104-7e20-4353-8330-1b88d4eb5a77" msprop:DbColumnName="SaleType" msprop:FriendlyName="SaleType" minOccurs="0">
                    <xs:simpleType>
                      <xs:restriction base="xs:string">
                        <xs:maxLength value="50" />
                      </xs:restriction>
                    </xs:simpleType>
                  </xs:element>
                  <xs:element name="Price" msprop:design-time-name="064ed318-b368-4645-bdaa-4daf9b9ccac6" msprop:DbColumnName="Price" msprop:FriendlyName="Price" type="xs:decimal" minOccurs="0" />
                  <xs:element name="TransactionDate" msprop:design-time-name="16df3b2b-19d0-41a4-a332-00945a8d5977" msprop:DbColumnName="TransactionDate" type="xs:int" minOccurs="0" />
                  <xs:element name="Id" msprop:design-time-name="2880ff34-c128-409a-a537-e0216c07fce5" msprop:DbColumnName="Id" type="xs:int" minOccurs="0" />
                  <xs:element name="GrossProfit" msprop:design-time-name="4853b722-68c8-44d1-8dae-595f389eb931" msprop:DbColumnName="GrossProfit" type="xs:decimal" minOccurs="0" />
                  <xs:element name="Department" msprop:design-time-name="6732cf1e-6514-4f78-9c09-032332b4ce70" msprop:DbColumnName="Department" minOccurs="0">
                    <xs:simpleType>
                      <xs:restriction base="xs:string">
                        <xs:maxLength value="100" />
                      </xs:restriction>
                    </xs:simpleType>
                  </xs:element>
                  <xs:element name="Division" msprop:design-time-name="baea94e7-c601-43d6-a99e-6cf718751e98" msprop:DbColumnName="Division" minOccurs="0">
                    <xs:simpleType>
                      <xs:restriction base="xs:string">
                        <xs:maxLength value="100" />
                      </xs:restriction>
                    </xs:simpleType>
                  </xs:element>
                </xs:sequence>
